// VectorOp:24
	vec2 n_out24p0 = n_out20p0 * vec2(n_out25p0);


// MultiplyAdd:28
	vec2 n_out28p0 = fma(n_out18p0, n_out22p0, n_out24p0);


// Texture2D:2
	vec4 n_out2p0 = texture(tex_frg_2, n_out28p0);


// VectorDecompose:3
	float n_out3p0 = n_out2p0.x;
	float n_out3p1 = n_out2p0.y;
	float n_out3p2 = n_out2p0.z;
	float n_out3p3 = n_out2p0.w;


	vec4 n_out30p0;
// Texture2D:30
	n_out30p0 = texture(Texture2DParameter, UV);
	float n_out30p1 = n_out30p0.r;


// FloatOp:31
	float n_out31p0 = n_out3p3 * n_out30p1;


// Output:0
	ALBEDO = vec3(n_out2p0.xyz);
	ALPHA = n_out31p0;


}
