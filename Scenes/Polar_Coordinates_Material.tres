// VectorDecompose:5
	float n_out5p0 = n_out4p0.x;
	float n_out5p1 = n_out4p0.y;
	float n_out5p2 = n_out4p0.z;
	float n_out5p3 = n_out4p0.w;


// VectorCompose:22
	vec2 n_out22p0 = vec2(n_out5p0, n_out5p1);


// VectorCompose:20
	vec2 n_out20p0 = vec2(n_out5p2, n_out5p3);


// Input:25
